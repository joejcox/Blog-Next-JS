{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020, 
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended" 
  ],
    "plugins": [
      "react",
      "react/recommended",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    },
    "globals": {
      "React": "writable"
    }
  }